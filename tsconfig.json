{
  "compilerOptions": {
  
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
    "@mui/styled-engine": ["./node_modules/@mui/styled-engine-sc"]

    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/components/theme/customizations/index.js", "src/components/dashboard/Dashboard.js", "src/components/Dashboard.tsx", "src/components/projects/Projects.js", "src/components/reports/Reports.js", "src/app/dashboard/page.tsx"],
  "exclude": ["node_modules"]
}
